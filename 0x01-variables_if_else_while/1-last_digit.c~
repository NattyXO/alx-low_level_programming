#include <stdlib.h>
#include <time.h>
#include <stdio.h>
#include <string.h>

int main(void)
{
  int n;

  srand(time(0));
  n = rand() - RAND_MAX / 2;
  int m = n[strlen(n)-1];
  if (m > 5)
    {
      printf(n, "%d and is freater than 5\n");
    }
  else if (m == 0)
    {
      printf(n , "%d and is 0\n");
    }
  else if (m < 6 && != 0)
    {
      printf(n, "%d and is less than 6 and not 0\n");
    }
  return (0);
}
